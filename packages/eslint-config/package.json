{
  "name": "@davidwarrington/eslint-config",
  "version": "2.6.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./rule-configs/*": {
      "types": "./dist/rule-configs/*.d.ts",
      "default": "./dist/rule-configs/*.js"
    }
  },
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidwarrington/tool-configs.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/davidwarrington/tool-configs/issues"
  },
  "keywords": [
    "eslint",
    "eslintrc"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "typescript-eslint": "^8.51.0"
  },
  "devDependencies": {
    "@eslint/core": "^1.0.0",
    "@tsconfig/recommended": "^1.0.13",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": "^9.39.2"
  }
}
