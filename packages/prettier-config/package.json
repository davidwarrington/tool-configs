{
  "name": "@davidwarrington/prettier-config",
  "version": "2.2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    },
