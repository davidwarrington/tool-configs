{
  "name": "@davidwarrington/stylelint-config",
  "version": "2.4.4",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./bemit": {
      "types": "./bemit/index.d.ts",
      "default": "./bemit/index.js"
    },
    "./scss": {
      "types": "./scss/index.d.ts",
      "default": "./scss/index.js"
    }
  },
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidwarrington/tool-configs.git",
    "directory": "packages/stylelint-config"
  },
  "bugs": {
    "url": "https://github.com/davidwarrington/tool-configs/issues"
  },
  "keywords": [
    "stylelint",
    "stylelintrc"
  ],
  "files": [
    "**/index.d.ts",
    "**/*.js"
  ],
  "scripts": {
    "build": "rm -f index.d.ts **/index.d.ts && tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "dependencies": {
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0"
  },
  "devDependencies": {
    "stylelint": "^16.25.0",
    "tsup": "^8.5.0",
    "vitest": "^3.2.4",
    "vitest-stylelint-utils": "^2.0.0"
  },
  "peerDependencies": {
    "stylelint": "^16.25.0"
  }
}
